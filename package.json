    "start:snc": "set PROVIDER=dreamspin&&set RNG=node_modules\\@slotify\\gdk\\lib\\rng\\SlotifyRNG&&set GAMES_PATH=games/space-hive-7/index.ts && ts-node-dev testsnc.js"
  },
